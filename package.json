    "gatsby-plugin-emotion": "^6.8.0",
    "gatsby-plugin-gatsby-cloud": "^3.0.0",
    "gatsby-plugin-manifest": "^3.13.0",
    "gatsby-plugin-mdx": "^2.8.0",
